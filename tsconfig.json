{
	"compilerOptions": {
		"lib": ["DOM", "ES5", "ES6", "ES7"],
		"inlineSourceMap": true,
		"inlineSources": true,
		"module": "ESNext",
		"target": "ES6",
		"allowJs": true,
		"noImplicitAny": true,
		"moduleResolution": "node",
		"importHelpers": true,
		"isolatedModules": true,
		"strictNullChecks": true,
		"strict": true,
		"stripInternal": true,
		"noEmit": true,
	}
}
